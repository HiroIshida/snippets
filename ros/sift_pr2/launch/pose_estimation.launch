<launch>
  <rosparam param="/use_sim_time">true</rosparam>
  <node pkg="rosbag" type="play" name="player" output="screen" args="-l --clock $(find sift_pr2)/preparation/estimation.bag"/>

  <node name="image_view"
        pkg="image_view" type="image_view">
